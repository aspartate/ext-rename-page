{
    "manifest_version": 3,
    "name": "Rename Page",
    "version": "1.0",
    "description": "Rename the current webpage title",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_security_policy": {}
  }
  